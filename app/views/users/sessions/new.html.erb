<% presenter = Users::Sessions::NewPresenter.new(self) %>

<% content_for(:head) do %>
  <title><%= presenter.page_title %></title>
  <%= stylesheet_pack_tag 'UserSessionNewPack', media: 'all' %>
<% end %>

<% content_for(:tail) do %>
  <%= javascript_pack_tag 'UserSessionNewPack', nonce: true %>
<% end %>

<div id="user-session-new" data-json-props="<%= presenter.props_to_json %>"></div>
