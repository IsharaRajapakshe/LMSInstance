<% presenter = Communities::NewQuestionPresenter.new(self, @community, @target) %>

<% content_for(:head) do %>
  <title><%= presenter.page_title %></title>
  <%= stylesheet_pack_tag 'QuestionsEditorPack', media: 'all' %>
<% end %>

<% content_for(:tail) do %>
  <%= javascript_pack_tag 'QuestionsEditorPack', nonce: true %>
<% end %>

<div class="w-full" id="questions-editor" data-json-props="<%= presenter.json_props %>"></div>
